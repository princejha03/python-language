num=int(input("enter number: "))
flag=1
if num == 0 or num == 1:
    print("The number is not prime:")
else:
    for i in range(2,num //2+1):
        if num % i == 0:
            flag = 0
            break
        if flag:
            print("The number is prime.")
        else:
            print("The number is not prime.")
    
